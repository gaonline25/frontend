{
  "redirects": [
    {
      "source": "/:path+/",
      "has": [
        {
          "type": "host",
          "value": "www.goldfingersaesthetics.com"
        }
      ],
      "destination": "https://goldfingersaesthetics.com/:path+",
      "permanent": true
    },
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.goldfingersaesthetics.com"
        }
      ],
      "destination": "https://goldfingersaesthetics.com/:path*",
      "permanent": true
    }
  ]
}
